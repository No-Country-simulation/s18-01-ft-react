{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env FORCE_COLOR=1 vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{js,jsx,json}\" \"./*.{js,cjs,json}\" --fix",
    "format": "prettier --check \"src/**/*.{js,jsx,json}\" \"./*.{js,cjs,json}\" --ignore-path .prettierignore ",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,json}\" \"./*.{js,cjs,json}\"  --ignore-path .prettierignore .",
    "commitlint:last": "npx commitlint --from HEAD~1 --to HEAD --verbose || $1",
    "prepare": "node setup-husky.js",
    "postinstall": "node setup-husky.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "prettier --write"
    ],
    "./*.{js,mjs,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.26.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.11.1",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react-swc": "3.7.1",
    "autoprefixer": "10.4.20",
    "cross-env": "7.0.3",
    "cssnano": "7.0.6",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.12",
    "eslint-plugin-tailwindcss": "3.17.4",
    "globals": "15.10.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "tailwindcss": "3.4.13",
    "vite": "5.4.8"
  }
}
